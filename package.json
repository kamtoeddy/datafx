{
  "author": "blacksocks",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.6.2"
  },
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/(*.)+(test).(ts|js)"
    ],
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ],
    "verbose": true
  },
  "license": "ISC",
  "main": "dist/index.js",
  "name": "datafx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamtoeddy/datafx.git"
  },
  "scripts": {
    "build": "tsc",
    "clean-up": "rm -rf coverage dist",
    "test": "tsc & jest --verbose --watchAll",
    "test-c": "tsc & jest --verbose --watchAll --coverage",
    "test:ci": "tsc & jest --verbose",
    "prepare": "npm run clean-up && npm run build"
  },
  "types": "dist/index.d.ts",
  "version": "0.2.8"
}
