{
  "author": "blacksocks",
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^4.6.2",
    "vitest": "^0.34.5"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "license": "ISC",
  "name": "datafx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamtoeddy/datafx.git"
  },
  "scripts": {
    "build": "tsup",
    "clean-up": "rm -rf coverage dist",
    "lint": "eslint . --ext .ts",
    "test": "npm run build && vitest run",
    "tes:cov": "npm run build && vitest run --coverage",
    "test:watch": "npm run build && vitest",
    "prepare": "npm run clean-up && npm run build"
  },
  "version": "0.2.10"
}
