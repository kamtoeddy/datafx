{
  "author": "",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.6.2"
  },
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/(*.)+(test).(ts|js)"
    ],
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "license": "ISC",
  "main": "dist/index.js",
  "name": "datafx",
  "scripts": {
    "build": "tsc",
    "test": "tsc & jest --verbose --watchAll",
    "test:ci": "tsc & jest --verbose",
    "prepare": "npm run build"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.17"
}
